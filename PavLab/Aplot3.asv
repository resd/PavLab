function [ output_args ] = Aplot3( class1, class2, class3, pr )
%APLOT3 Summary of this function goes here
%   Detailed explanation goes here
switch pr
    case 3
        plot3(class1(:, 1), class1(:, 2), class1(:, 3), 'b+');
        hold on;
        plot3(class2(:, 1), class2(:, 2), class2(:, 3), 'ro');
    case 2
        plot(class1(:, 1), class1(:, 2), 'b+');
        hold on;
        plot(class2(:, 1), class2(:, 2), 'ro');
end

end

